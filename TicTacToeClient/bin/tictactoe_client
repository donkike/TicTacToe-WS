#!/usr/bin/env ruby
$LOAD_PATH.push File.join(File.dirname(__FILE__), "/../lib")
require 'tictactoe_client'

player = TicTacToeClient::Player.new(ARGV.shift, ARGV.shift)
player.register

puts "Welcome #{player.name}!"
puts "Type help for a list of available commands.\n\n"

menu = TicTacToeClient::Menu.new(player)
menu.begin
    


